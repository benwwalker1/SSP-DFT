&control
    calculation = 'bands'
    restart_mode='from_scratch',
    prefix='graphene',
    pseudo_dir = './',
    outdir='./tmp/'
    /
&system
    ibrav = 4, 
    celldm(1) = 4.65,
    celldm(3) = 4,
    nat= 2, 
    ntyp= 1,
    ecutwfc = 75.0
    nbnd=10,
    /
&electrons
    diagonalization='david'
    conv_thr =  1.0d-8
    /
